{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "es6"],
    "allowJs": true,
    "jsx": "react",
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}
